{
  "cli": {
    "version": ">= 5.9.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_TOKEN_BASE_URL": "https://api.finuncle.com/token",
        "EXPO_PUBLIC_USER_BASE_URL": "https://api.finuncle.com/users/api",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.finuncle.com/api/v1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_TOKEN_BASE_URL": "https://api.finuncle.com/token",
        "EXPO_PUBLIC_USER_BASE_URL": "https://api.finuncle.com/users/api",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.finuncle.com/api/v1"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_TOKEN_BASE_URL": "https://api.finuncle.com/token",
        "EXPO_PUBLIC_USER_BASE_URL": "https://api.finuncle.com/users/api",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.finuncle.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "bharathkumarchowdary006@gmail.com",
        "ascAppId": "6475839395",
        "appleTeamId": "8XRCQ5TVHW"
      }
    }
  }
}
